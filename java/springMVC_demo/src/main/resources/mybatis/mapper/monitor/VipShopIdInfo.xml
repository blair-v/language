<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.x.dmt.dao.monitor.VipShopIdInfoDAO">

    <resultMap id="ShopIdInfoResultMap" type="ShopIdInfo" autoMapping="true">
        <result column="shop_id" property="shopId"></result>
    </resultMap>

    <select id="getVipShopIdInfos" resultMap="ShopIdInfoResultMap">
        <![CDATA[
            select
              DISTINCT shop_id
            from monitor_shop
        ]]>
    </select>
</mapper>